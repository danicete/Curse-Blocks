$zindex-counter: 0 !default;

@while $zindex-counter < 1 {

    // Utility -> z-index (u-zindex-a)

    // ✎ Standard project z-index

    // ⌚ Level 1.00 (02/017/12)

    // ☍ Components:
    //     - Null

    // ☆ Sub-Classes:
    //     - Null

    // ☯ Browser Support:
    //     - Testing Required
 
    // -_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_

    #netbar { position: relative; z-index: 4; }

    header.main { position: relative; z-index: 3; }

    .u-ad-leaderboard,
    .ad-leaderboard,
    .ad-med-rect { position: relative; z-index: 2; }

    #content { position: relative; z-index: 1; }

    $zindex-counter: $zindex-counter + 1;
}